{
  "name": "ruby-on-rails-repo-template",
  "version": "1.0.0",
  "description": "Ruby on rails 6.0 Already generated Repo Template for Github",
  "private": true,
  "scripts": {
    "lint:rubocop": "rubocop",
    "lint:haml": "haml-lint app/views/**/*",
    "lint:js": "eslint app/**/*.js",
    "lint:css": "stylelint app/**/*.scss",
    "lint": "npm-run-all lint:*",
    "dev:start": "foreman start -f Procfile.dev",
    "test:start": "bundle exec rake test",
    "test:open": "open coverage/index.html",
    "test": "npm-run-all test:*"
  },
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/chiefpansancolt/ruby-on-rails-repo-template"
  },
  "license": "MIT",
  "author": {
    "name": "Christopher Pezza",
    "email": "chiefpansancolt@gmail.com"
  },
  "contributors": [
    {
      "name": "Christopher Pezza",
      "email": "chiefpansancolt@gmail.com"
    }
  ],
  "dependencies": {
    "@rails/webpacker": "^4.0.2",
    "actioncable": "^5.2.2",
    "material-components-web": "^1.0.1",
    "material-design-icons-iconfont": "^5.0.1",
    "rails-erb-loader": "^5.5.2",
    "resolve-url-loader": "^3.1.0",
    "sweetalert2": "^8.12.1",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "npm-run-all": "^4.1.5",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4",
    "webpack-dev-server": "^3.2.1"
  }
}
